include <LiquidCrystal.h>
LiquidCrystal lcd(12,11,5,4,3,2);
void setup()
{
lcd.begin(16, 2);
}

void loop()
{
  lcd.setCursor(0, 0);
  lcd.print(" I love you....");
  lcd.setCursor(0, 1);
  lcd.print("Maram");
}
// if u want to have look on LCD connection with arduino, visit this link: https://www.tinkercad.com/things/4NAvVNWYEJs-lcd/editel?sharecode=9jNl8NDTGpzMAvr-qiBOFIKiq2cdGuOZzcgxCYhRr3Y
